@article{Kaiser.1999,
	abstract = {Das Strippen ist ein mit Luft oder Wasserdampf betriebenes Ausgasungsverfahren. Es eignet sich zur einfachen Entfernung von Ammonium-Stickstoff aus Abwasser. Die Grundlagen des Verfahrens werden vorgestellt und dann Praxisbeispiele gezeigt. Das Strippen verlagert das Schadstoffproblem von der Fl{\"u}ssigphase in die Gas- oder Dampfphase. Doch durch eine Nachbehandlung des Strippgases ist eine R{\"u}ckgewinnung des Ammoniaks als Wertstoff m{\"o}glich. Ein Anlagenschema zur Ammoniak-Strippung ist abgebildet. Damit Ammoniak in Hydratform strippbar wird, mu{\ss} das Abwasser mit Kalkmilch alkalisiert werden; ein Diagramm zeigt das Dissoziationsgleichgewicht von Ammoniak in Wasser. Die Strippung erfolgt in einer F{\"u}llk{\"o}rperkolonne im Gegenstromverfahren. Die beladenen Br{\"u}den werden am Kolonnenkopf abgezogen. Die Dampfstrippung erzeugt keine Reststoffe und zeichnet sich durch einen hohen Wirkungsgrad aus. Die Luftstrippung bietet sich vor allem dann an, wenn kein betriebseigenes Dampf- und K{\"u}hlwassernetz zur Verf{\"u}gung steht. Zur Br{\"u}denbehandlung bietet sich an die R{\"u}ckgewinnung von Ammoniakwasser, Abluftw{\"a}sche mit anschlie{\ss}ender Verwertung und Verbrennung. Das durch Dampfstrippung erhaltene Wertprodukt Ammoniakwasser, frei von Reststoffen, kann z.B. zur nicht-katalytischen Entstickung eingesetzt werden. Bei der Luftstrippung kann die beladene Abluft verbrannt werden (bei kleinen Volumenstr{\"o}men). Bei der Abgasw{\"a}sche wird mittels S{\"a}uren Ammoniumsalzl{\"o}sung gewonnen, die als Fl{\"u}ssigd{\"u}nger verwendet werden kann. Als Beispiele industrieller Anlagen werden beschrieben: Ammoniakentfernung aus Rauchgas-Waschwasser mittels Dampfstrippung, Luftstrippung als Teilstrombehandlung in Kl{\"a}ranlagen, Dampfstrippkolonne im Raffineriebereich.},
	author = {Kaiser, S. and Lehner, M.},
	year = {1999},
	title = {{Wertstoffe gewinnen. Dampf- und Luftstrippung zur R{\"u}ckgewinnung von Ammoniak aus Abwasser}},
	keywords = {Abwasserbehandlung;Alkalisierung;Ammoniak;Ammoniumverbindung;Ausgasen;Dampfeinleitung;Denitrifikation;Dissoziation;Industrieabwasser;Lufteinlauf;Trennkolonne;w{\"a}ssrige L{\"o}sung;Wertstoffr{\"u}ckgewinnung},
	pages = {20--22},
	volume = {33},
	number = {3},
	issn = {0175-5315},
	journal = {Verfahrenstechnik}
}

